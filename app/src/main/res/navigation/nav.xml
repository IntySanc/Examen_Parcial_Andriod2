<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/menu">

    <fragment
        android:id="@+id/menu"
        android:name="com.example.examen_parical.menu"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu" >
        <action
            android:id="@+id/go_cuadrado"
            app:destination="@id/cuadro" />
        <action
            android:id="@+id/go_raiz"
            app:destination="@id/raiz" />
        <action
            android:id="@+id/go_elevadoCubo"
            app:destination="@id/elevadoCubo" />
        <action
            android:id="@+id/go_antecesor"
            app:destination="@id/antecesor" />
    </fragment>
    <fragment
        android:id="@+id/cuadro"
        android:name="com.example.examen_parical.Cuadro"
        android:label="fragment_cuadro"
        tools:layout="@layout/fragment_cuadro" />
    <fragment
        android:id="@+id/raiz"
        android:name="com.example.examen_parical.Raiz"
        android:label="fragment_raiz"
        tools:layout="@layout/fragment_raiz" />
    <fragment
        android:id="@+id/elevadoCubo"
        android:name="com.example.examen_parical.ElevadoCubo"
        android:label="fragment_elevado_cubo"
        tools:layout="@layout/fragment_elevado_cubo" />
    <fragment
        android:id="@+id/antecesor"
        android:name="com.example.examen_parical.Antecesor"
        android:label="fragment_antecesor"
        tools:layout="@layout/fragment_antecesor" />
</navigation>